<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="700" prefWidth="700" xmlns="http://javafx.com/javafx/25" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.gpa_calculator.Result">
    <children>
        <Label layoutX="305.0" layoutY="20.0" prefHeight="27.0" prefWidth="126.0" text="   All Students GPA" textFill="#343498">
         <font>
            <Font name="Britannic Bold" size="14.0" />
         </font></Label>
        <TableView fx:id="table" layoutX="50" layoutY="60" prefHeight="500" prefWidth="600">
            <columns>
                <TableColumn fx:id="colName" prefWidth="200" text="Name" />
                <TableColumn fx:id="colRoll" prefWidth="150" text="Roll" />
                <TableColumn fx:id="colGPA" prefWidth="100" text="GPA" />
                <TableColumn fx:id="colActions" prefWidth="150" text="Actions" />
            </columns>
        </TableView>
        <Button layoutX="105.0" layoutY="593.0" onAction="#restart" text="Add New Student" textFill="#19b52e">
         <font>
            <Font name="Bodoni MT Italic" size="14.0" />
         </font></Button>
        <Button layoutX="493.0" layoutY="593.0" onAction="#exitApp" prefHeight="27.0" prefWidth="75.0" text="Exit" textFill="#db0b0b">
         <font>
            <Font name="Calisto MT Italic" size="14.0" />
         </font></Button>
    </children>
</AnchorPane>
