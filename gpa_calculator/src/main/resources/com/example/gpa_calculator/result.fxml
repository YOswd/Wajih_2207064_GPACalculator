<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="700" prefWidth="700" xmlns="http://javafx.com/javafx/25"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.gpa_calculator.Result">
    <children>
        <Label text="All Students GPA" layoutX="250" layoutY="20"/>
        <TableView fx:id="table" layoutX="50" layoutY="60" prefHeight="500" prefWidth="600">
            <columns>
                <TableColumn fx:id="colName" text="Name" prefWidth="200"/>
                <TableColumn fx:id="colRoll" text="Roll" prefWidth="150"/>
                <TableColumn fx:id="colGPA" text="GPA" prefWidth="100"/>
                <TableColumn fx:id="colActions" text="Actions" prefWidth="150"/>
            </columns>
        </TableView>
        <Button text="Add New Student" layoutX="100" layoutY="580" onAction="#restart"/>
        <Button text="Exit" layoutX="500" layoutY="580" onAction="#exitApp"/>
    </children>
</AnchorPane>
